<Page class="page">
	<Page.actionBar>
		<ActionBar title="HTTPS Playground" class="action-bar"></ActionBar>
	</Page.actionBar>

	<StackLayout>
		<Button text="Test HTTPS" tap="getRequest" class="btn btn-primary btn-active" />
		<Button text="Enable SSL" tap="enableSSL" class="btn btn-primary btn-active" />
		<Button text="Disable SSL" tap="disableSSL" class="btn btn-primary btn-active" />
	</StackLayout>

</Page>
